<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageId>Carbon.UMAP</PackageId>
    <TargetFrameworks>net8.0</TargetFrameworks>
    <RootNamespace>Carbon.UMAP</RootNamespace>
    <AssemblyName>Carbon.UMAP</AssemblyName>
    <Authors>Curiosity GmbH, Jason Nelson</Authors>
    <Company>Curiosity GmbH</Company>
    <Product />
    <Description>C# implementation of "Uniform Manifold Approximation and Projection" (UMAP) for embedding projections.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/curiosity-ai/umap-sharp</PackageProjectUrl>
    <RepositoryUrl>https://github.com/curiosity-ai/umap-sharp</RepositoryUrl>
    <PackageTags>UMAP, embeddings projection, dimensionality reduction, TSNE, data science, embeddings, word2vec</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Copyright>(c) Copyright 2022 Curiosity GmbH - all right reserved</Copyright>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
  </PropertyGroup>


</Project>
